<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StickyNavigationLayout">
         <attr name="stickyLayout_top_id" format="integer|reference"/>
         <attr name="stickyLayout_indicator_id" format="integer|reference"/>
         <attr name="stickyLayout_content_id" format="integer|reference"/>
         <attr name="stickyLayout_auto_fit_scroll" format="boolean|reference"/>
         <attr name="stickyLayout_threshold_percent" format="float|reference"/>
    </declare-styleable>

    <declare-styleable name="NestedScrollFrameLayout">
        <!-- deprecated -->
        <attr name="nsfl_max_y_percent"  format="float|reference"/>
        <!-- added in '1.0.3'  -->
        <attr name="nsfl_max_percent"  format="float|reference"/>
        <attr name="nsfl_orientation"  format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
    </declare-styleable>
</resources>
